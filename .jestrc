{
    "verbose": true,
    "bail": true,
    "automock": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testRegex": "\\.test\\.js$",
    "unmockedModulePathPatterns": ["mocks", "colors"],
    "coveragePathIgnorePatterns": ["/node_modules/", "/mocks/"],
    "clearMocks": true,
    "moduleNameMapper": {
        "package\\.json$": "<rootDir>/tests/mocks/package.js"
    },
    "testEnvironment": "node"
}
